import java.util.*;
class Mai{
  public static void main(String args[]){
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int a[]=new int[n];
        int b=0;
        int c=0;
        for(int i=0;i<n;i++){
            a[i]=sc.nextInt();
        }
        for(int i=0;i<n;i++){
            b=b*10+a[i];
            
            
        }
       while(b>0){
        c=b%10;
        b=b/10;
         System.out.print(c);
       }
    }
}
